{
  "name": "@xrpckit/cli",
  "version": "0.0.2",
  "description": "CLI for xRPC - generate type-safe API code from Zod schemas",
  "type": "module",
  "license": "MIT",
  "author": "mwesox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwesox/xrpc.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/mwesox/xrpc/issues"
  },
  "homepage": "https://github.com/mwesox/xrpc#readme",
  "keywords": ["xrpc", "rpc", "cli", "typescript", "codegen", "api", "type-safe", "zod"],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "xrpc": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "@xrpckit/sdk": "workspace:*",
    "@xrpckit/target-go-server": "workspace:*",
    "@xrpckit/target-swift-client": "workspace:*",
    "@xrpckit/target-ts-server": "workspace:*",
    "@xrpckit/target-ts-client": "workspace:*",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "ora": "^8.0.0",
    "smol-toml": "^1.3.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
